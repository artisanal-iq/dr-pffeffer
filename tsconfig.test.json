{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./.test-dist",
    "noEmit": false,
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "ES2019",
    "declaration": false,
    "emitDeclarationOnly": false,
    "types": ["node"],
    "esModuleInterop": true,
    "allowJs": false
  },
  "include": [
    "middleware.ts",
    "src/lib/**/*.ts",
    "src/__tests__/**/*.ts"
  ]
}
